{
  "name": "AUM extension",
  "version": "0.9.1",
  "description": "Makes possible to open profiles in tabs on www.adopteunmec.com (adopte un mec)",
  "icons": {"128": "icon-128.png"},
  "background_page": "background.html",
  "permissions": ["tabs"],
  "page_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://www.adopteunmec.com/home.php"],
      "js": ["jquery.js", "aum.js"],
      "all_frames": true
    }
  ]
}
